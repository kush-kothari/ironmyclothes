{
  "name": "irontrack",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "copy:sqlwasm": "node -e \"const fs=require('fs');const path=require('path');const d=path.join('public','assets');if(!fs.existsSync(d))fs.mkdirSync(d,{recursive:true});fs.copyFileSync(path.join('node_modules','sql.js','dist','sql-wasm.wasm'),path.join(d,'sql-wasm.wasm'));\"",
    "predev": "npm run copy:sqlwasm",
    "prebuild": "npm run copy:sqlwasm",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cap:init": "cap init",
    "cap:sync": "npm run build && cap sync",
    "cap:run:android": "cap run android"
  },
  "dependencies": {
    "sql.js": "1.11.0",
    "@capacitor/android": "^7.0.0",
    "@capacitor/camera": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/filesystem": "^7.0.0",
    "@capacitor-community/sqlite": "^7.0.3",
    "lucide-react": "^0.460.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.0.0",
    "@eslint/js": "^9.13.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^6.0.0"
  },
  "capacitor": {
    "appId": "com.irontrack.app",
    "appName": "IronTrack",
    "webDir": "dist"
  },
  "overrides": {
    "sql.js": "1.11.0"
  }
}
